    <!-- Wifi -->
    <header
        android:id="@+id/wifi_settings"
        android:fragment="com.android.settings.wifi.WifiSettings"
        android:title="@string/wifi_settings_title"
        android:icon="@drawable/ic_settings_wireless_hw" />

    <!-- Bluetooth -->
    <header
        android:id="@+id/bluetooth_settings"
        android:fragment="com.android.settings.bluetooth.BluetoothSettings"
        android:title="@string/bluetooth_settings_title"
        android:icon="@drawable/ic_settings_bluetooth2_hw" />

    <!-- Brightness  -->
    <header
    	android:id="@+id/brightness_settings"
    	android:title="@string/brightness"
    	android:icon="@drawable/ic_settings_brightness_hw" >
	<intent
        	android:targetPackage="com.android.settings"
        	android:targetClass="com.android.settings.BrightnessPreferenceActivity"/>
    	</header>

    <!-- Wallpaper  -->
    <header
    	android:id="@+id/wallpaper_settings"
    	android:fragment="com.android.settings.WallpaperTypeSettings"
    	android:title="@string/wallpaper_settings_title"
    	android:icon="@drawable/ic_settings_wallpaper_hw" />

    <!-- Font -->
    <header
        android:id="@+id/display_settings"
        android:icon="@drawable/ic_settings_display_hw"
        android:fragment="com.android.settings.DisplaySettings"
        android:title="@string/hw_font" >
        <intent
            android:action="com.android.settings.font_settings"
            />
    </header>

